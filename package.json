{
  "name": "siyuan-plugin-picgo",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "serve": "vite",
    "dev": "pnpm pluginBuild && pnpm siyuanSync && pnpm siyuanDev",
    "build": "pnpm siyuanBuild && pnpm pluginBuild && pnpm siyuanSync",
    "pluginBuild": "zhi-build --production",
    "siyuanDev": "vue-tsc && vite build --watch",
    "siyuanBuild": "vue-tsc && vite build",
    "siyuanSync": "cp -r ./dist/* /Users/terwer/Documents/mydocs/SiYuanWorkspace/public/data/plugins/siyuan-plugin-picgo",
    "preview": "vite preview"
  },
  "devDependencies": {
    "@terwer/esbuild-config-custom": "latest",
    "@terwer/eslint-config-custom": "latest",
    "@vitejs/plugin-vue": "^4.1.0",
    "typescript": "^5.0.2",
    "vite": "^4.3.9",
    "vue-tsc": "^1.4.2"
  },
  "dependencies": {
    "siyuan": "^0.7.3",
    "vue": "^3.2.47",
    "zhi-lib-base": "^0.2.5"
  }
}
